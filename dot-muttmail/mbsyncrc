#################################
######## Account gcman105 #######
#################################

IMAPAccount gcman105
Host imap.ionos.co.uk
User gary@gcman105.com
PassCmd "security find-internet-password -s 'imap.ionos.co.uk' -a 'gary@gcman105.com' -w"
SSLType IMAPS
SSLVersions TLSv1.2
CertificateFile ~/dotfiles/dot-muttmail/certificates.crt

# Remote storage
IMAPStore gcman105-remote
Account gcman105

# Local storage
MaildirStore gcman105-local
Path ~/Mail/gcman105/
Inbox ~/Mail/gcman105/INBOX

Channel gcman105-inbox
Master :gcman105-remote:"Inbox"
Slave :gcman105-local:INBOX
Create Both
Expunge Both

Channel gcman105-archive
Master :gcman105-remote:"Archive"
Slave :gcman105-local:archive
Create Both
Expunge Both

Channel gcman105-drafts
Master :gcman105-remote:"Drafts"
Slave :gcman105-local:drafts
Create Both
Expunge Both

Channel gcman105-sent
Master :gcman105-remote:"Sent Items"
Slave :gcman105-local:sent
Create Both
Expunge Both

Channel gcman105-trash
Master :gcman105-remote:"Trash"
Slave :gcman105-local:trash
Create Both
Expunge Both

Channel gcman105-junk
Master :gcman105-remote:"Spam"
Slave :gcman105-local:junk
Create Both
Expunge Both

Group gcman105
Channel gcman105-inbox
Channel gcman105-archive
Channel gcman105-drafts
Channel gcman105-sent
Channel gcman105-trash
Channel gcman105-junk

