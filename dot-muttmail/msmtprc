account gcman105
host smtp.ionos.com
port 465
protocol smtp
auth on
user gary@gcman105.com
from gary@gcman105.com
passwordeval "security find-internet-password -s 'imap.ionos.co.uk' -a 'gary@gcman105.com' -w"
tls on
tls_starttls off
tls_trust_file ~/dotfiles/dot-muttmail/certificates.crt

